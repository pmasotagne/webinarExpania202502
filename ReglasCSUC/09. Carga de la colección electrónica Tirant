rule "Afegir 908 Tirant" 
when 
	(not exists "908") 
then 
	addField "908.a.UB"      
	addSubField "908.a.UAB" 
	addSubField "908.a.UPF" 
	addSubField "908.a.UdG" 
	addSubField "908.a.URL" 
	addSubField "908.a.URV"    
end


###################################
## 2. Creación de la 856 inicial ##
###################################

rule "Afegir 856 Tirant: UB" 
when 
	(exists "020.a") AND (exists "940.a.UB") 
then 
	addField "915.{4,0}.z.Accés restringit als usuaris de la UB" 
	addField "915.u.https://biblioteca.nubedelectura.com/cloudLibrary/login/login?username=CRAIUB&password=XXXXXX&redirectto=/ebook/show/" 
	suffixSubField "915.u" with "020.a" 
	combineFields "915" excluding "" 
	changeField "915" to "856" 
end 

rule "Afegir 856 Tirant: UAB" 
when 
	(exists "020.a") AND (exists "940.a.UAB") 
then 
	addField "915.{4,0}.z.Accés restringit als usuaris de la UAB" 
	addField "915.u.http://biblioteca.tirant.com/cloudLibrary/login/login?username=UAB&password=XXXXXX&redirectto=/ebook/show/" 
	suffixSubField "915.u" with "020.a" 
	combineFields "915" excluding "" 
	changeField "915" to "856" 
end 

rule "Afegir 856 Tirant: UPF" 
when 
	(exists "020.a") AND (exists "940.a.UPF") 
then 
	addField "915.{4,0}.z.Accés restringit als usuaris de la UPF" 
	addField "915.u.https://bibticapps.upf.edu/tirant/auth.php?isbn=" 
	suffixSubField "915.u" with "020.a" 
	combineFields "915" excluding "" 
	changeField "915" to "856" 
end 

rule "Afegir 856 Tirant: UdG" 
when 
	(exists "020.a") AND (exists "940.a.UdG") 
then 
	addField "915.{4,0}.z.Accés restringit als usuaris de la UdG" 
	addField "915.u.https://biblioteca.tirant.com/cloudLibrary/login/login?username=UGIRONA&password=XXXXXX&redirectto=/ebook/show/" 
	suffixSubField "915.u" with "020.a" 
	combineFields "915" excluding "" 
	changeField "915" to "856" 
end 

rule "Afegir 856 Tirant: URL" 
when 
	(exists "020.a") AND (exists "940.a.URL") 
then 
	addField "915.{4,0}.z.Accés restringit als usuaris de la URL" 
	addField "915.u.https://biblioteca.tirant.com/cloudLibrary/login/login?username=ESADE&password=XXXXXX&redirectto=/ebook/show/" 
	suffixSubField "915.u" with "020.a" 
	combineFields "915" excluding "" 
	changeField "915" to "856" 
end 

rule "Afegir 856 Tirant: URV" 
when 
	(exists "020.a") AND (exists "940.a.URV") 
then 
	addField "915.{4,0}.z.Accés restringit als usuaris de la URV" 
	addField "915.u.https://biblioteca.tirant.com/cloudLibrary/login/login?username=URV&password=XXXXXX&redirectto=/ebook/show/" 
	suffixSubField "915.u" with "020.a" 
	combineFields "915" excluding "" 
	changeField "915" to "856" 
end


#################################################################
## 3. Creación de las etiquetas 856 del resto de instituciones ##
#################################################################

rule "Afegir 856 Tirant - UB" 
when 
	exists "856.z.*UB*" 
then 
	copyField "856" to "915" if (exists "856.z.*UB*") 
	replaceContents "915.z.Accés restringit als usuaris de la UB" with "Accés restringit als usuaris de la UAB" 
	replaceContents "915.u.https://biblioteca.nubedelectura.com/cloudLibrary/login/login?username=CRAIUB&password=XXXXXX&redirectto=/ebook/show/" with "http://biblioteca.tirant.com/cloudLibrary/login/login?username=UAB&password=8zM8jgWE&redirectto=/ebook/show/" 
	changeField "915" to "856" 
	copyField "856" to "915" if (exists "856.z.*UB*") 
	replaceContents "915.z.Accés restringit als usuaris de la UB" with "Accés restringit als usuaris de la UPF" 
	replaceContents "915.u.https://biblioteca.nubedelectura.com/cloudLibrary/login/login?username=CRAIUB&password=XXXXXX&redirectto=/ebook/show/" with "https://bibticapps.upf.edu/tirant/auth.php?isbn=" 
	changeField "915" to "856" 
	copyField "856" to "915" if (exists "856.z.*UB*") 
	replaceContents "915.z.Accés restringit als usuaris de la UB" with "Accés restringit als usuaris de la UdG" 
	replaceContents "915.u.https://biblioteca.nubedelectura.com/cloudLibrary/login/login?username=CRAIUB&password=XXXXXX&redirectto=/ebook/show/" with "https://biblioteca.tirant.com/cloudLibrary/login/login?username=UGIRONA&password=YB6K1vL&redirectto=/ebook/show/" 
	changeField "915" to "856" 
	copyField "856" to "915" if (exists "856.z.*UB*") 
	replaceContents "915.z.Accés restringit als usuaris de la UB" with "Accés restringit als usuaris de la URL" 
	replaceContents "915.u.https://biblioteca.nubedelectura.com/cloudLibrary/login/login?username=CRAIUB&password=XXXXXX&redirectto=/ebook/show/" with "https://biblioteca.tirant.com/cloudLibrary/login/login?username=ESADE&password=m3FirqTL&redirectto=/ebook/show/" 
	changeField "915" to "856" 
	copyField "856" to "915" if (exists "856.z.*UB*") 
	replaceContents "915.z.Accés restringit als usuaris de la UB" with "Accés restringit als usuaris de la URV" 
	replaceContents "915.u.https://biblioteca.nubedelectura.com/cloudLibrary/login/login?username=CRAIUB&password=XXXXXX&redirectto=/ebook/show/" with "https://biblioteca.tirant.com/cloudLibrary/login/login?username=URV&password=N8FGKfP&redirectto=/ebook/show/" 
	changeField "915" to "856" 
end 
	
